<div class="paper-top"></div>
</section>

    <script>
        function toggleFaq(element) {
            const answer = element.nextElementSibling;
            const icon = element.querySelector('.faq-icon');
            const isOpen = answer.classList.contains('open');

            // Close all other FAQs
            document.querySelectorAll('.faq-answer.open').forEach(openAnswer => {
                if (openAnswer !== answer) {
                    openAnswer.classList.remove('open');
                    openAnswer.previousElementSibling.classList.remove('active');
                    openAnswer.previousElementSibling.querySelector('.faq-icon').textContent = '+';
                }
            });

            // Toggle current FAQ
            if (isOpen) {
                answer.classList.remove('open');
                element.classList.remove('active');
                icon.textContent = '+';
            } else {
                answer.classList.add('open');
                element.classList.add('active');
                icon.textContent = 'âˆ’';
            }
        }

        // Optional: Close FAQ when clicking outside
        document.addEventListener('click', function(event) {
            if (!event.target.closest('.faq-item')) {
                document.querySelectorAll('.faq-answer.open').forEach(answer => {
                    answer.classList.remove('open');
                    answer.previousElementSibling.classList.remove('active');
                    answer.previousElementSibling.querySelector('.faq-icon').textContent = '+';
                });
            }
        });
    </script>